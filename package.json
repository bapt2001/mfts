{
    "name": "MFTS",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --mode production --config webpack.prod.js",
        "source-map-explorer": "source-map-explorer ./dist/main.*.js ./dist/main.*.js.map",
        "stats": "yarn build && webpack-bundle-analyzer dist/.webpack-stats.json",
        "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config webpack.dev.js",
        "clean:all": "run-p clean:build clean:dist",
        "clean:build": "rimraf build",
        "clean:dist": "rimraf dist",
        "server:build": "next ./server build && tsc --project tsconfig.server.json",
        "server:dev": "nodemon server/index.ts",
        "server:start": "NODE_ENV=production node production-server/index.js",
        "tsc": "tsc -p . --noEmit",
        "lint": "tslint --project './tsconfig.json'",
        "lint:fix": "tslint --project './tsconfig.json' --fix",
        "prettier": "prettier --write \"./src/**/*.{ts,tsx,scss,js,json}\" \"./server/**/*.{ts,tsx,scss,js,json}\" --write \"./*.{js,json}\"",
        "test": "jest --config jest.config.json",
        "test:coverage": "jest --config jest.config.json --coverage",
        "test:watch": "jest --config jest.config.json --watch",
        "test:update": "jest --config jest.config.json -u"
    },
    "husky": {
        "hooks": {
            "post-commit": "git diff --name-only HEAD^ | xargs git add",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss}": [
            "prettier --write",
            "git add"
        ]
    },
