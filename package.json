{
  "name": "MFTS",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "build": "yarn clean:dist && cross-env NODE_ENV=production BABEL_ENV=production webpack --mode production --config webpack.prod.js",
    "source-map-explorer": "source-map-explorer ./dist/main.*.js ./dist/main.*.js.map",
    "stats": "yarn build && webpack-bundle-analyzer dist/.webpack-stats.json",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config webpack.dev.js",
    "clean:all": "run-p clean:build clean:dist",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "check": "npm run lint & npm run tsc",
    "lint": "tslint --project './tsconfig.json'",
    "lint:fix": "tslint --project './tsconfig.json' --fix",
    "prettier:ts": "prettier --write \"./src/js/**/*.{ts,tsx}\"",
    "tsc": "tsc -p . --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
