{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": false,
        "targets": {
          "browsers": [
            ">1%",
            "last 2 versions",
            "Firefox ESR",
            "not ie < 9",
            "ie 11",
            "last 3 ios versions",
            "last 3 safari versions",
            "last 4 chromeAndroid versions"
          ]
        },
        "debug": false
      }
    ],
    "react",
    "stage-3"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-es2015-spread"
  ]
  /*
